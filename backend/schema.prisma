// This file is automatically generated by Keystone, do not modify it manually.
// Modify your Keystone config when you want to change this.

datasource mysql {
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
  provider          = "mysql"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        String    @id @default(cuid())
  name      String    @default("")
  email     String    @unique @default("")
  password  String
  createdAt DateTime? @default(now())
}

model about {
  id          String @id @default(cuid())
  route       String @default("")
  title       String @default("")
  description String @default("") @mysql.VarChar(6000)
  link        String @default("")
}

model conferenceTheme {
  id    String @id @default(cuid())
  title String @default("") @mysql.VarChar(6000)
}

model organizingCommittee {
  id          String  @id @default(cuid())
  name        String  @default("")
  category    String?
  designation String  @default("")
  dept        String  @default("")
  desc        String  @default("")
  photoUrl    String  @default("")
}

model technicalCommittee {
  id          String @id @default(cuid())
  name        String @default("")
  designation String @default("")
  dept        String @default("")
  institute   String @default("")
}

model sponsorship {
  id               String  @id @default(cuid())
  type             String  @default("")
  amount           Int
  delegates        Int
  stallarea        Int     @default(0)
  backdropBanner   Boolean @default(false)
  presentationSlot Boolean @default(false)
  addvertisement   String  @default("")
}

model importantDates {
  id          String  @id @default(cuid())
  event       String  @default("")
  date        String  @default("")
  description String  @default("")
  important   Boolean @default(false)
}

model registrationDetails {
  id             String  @id @default(cuid())
  native         String?
  particulars    String  @default("")
  amt_early_bird Int
  amt_regular    Int
}

model speakers {
  id          String @id @default(cuid())
  type        String @default("")
  name        String @default("") @mysql.VarChar(500)
  designation String @default("") @mysql.VarChar(500)
  title       String @default("") @mysql.VarChar(500)
  bio         String @default("") @mysql.VarChar(6000)
  photoUrl    String @default("")
}

model toursit {
  id       String @id @default(cuid())
  name     String @default("")
  location String @default("")
  photoUrl String @default("")
  desc     String @default("") @mysql.VarChar(6000)
}

model tracks {
  id     String @id @default(cuid())
  number Int
  type   String @default("")
  name   String @default("")
}

model eventDetails {
  id       String @id @default(cuid())
  name     String @default("")
  desc     String @default("") @mysql.VarChar(6000)
  photoUrl String @default("")
  link     String @default("")
}

model generalInformation {
  id   String @id @default(cuid())
  name String @default("")
  desc String @default("") @mysql.VarChar(6000)
}

model subthemes {
  id     String @id @default(cuid())
  number Int
  desc   String @default("")
}

model notices {
  id        String    @id @default(cuid())
  title     String    @default("")
  desc      String    @default("") @mysql.VarChar(6000)
  date      String    @default("")
  isNew     Boolean   @default(false)
  createdAt DateTime? @default(now())
}
